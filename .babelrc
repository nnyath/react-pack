{
  "presets": [
    ["env", {
      "modules": false
    }],
    "react" ],
  "plugins": [
    "react-hot-loader/babel",
    "transform-object-rest-spread", 
    "transform-decorators-legacy", 
    "babel-plugin-transform-async-to-generator",
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "Actions": "./src/actions",
        "Containers": "./src/containers",
        "Components": "./src/components",
        "Reducers": "./src/reducers",
        "Redux": "./src/redux"
      }
    }]
  ]
}
